<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center_vertical" android:id="@android:id/background" android:height="2.0dip">
        <shape android:shape="rectangle">
            <corners android:radius="1.0dip" />
            <size android:height="2.0dip" />
            <solid android:color="#ffbfc2c2" />
        </shape>
    </item>
    <item android:gravity="center_vertical" android:id="@android:id/progress" android:height="2.0dip">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="1.0dip" />
                <size android:height="2.0dip" />
                <solid android:color="@color/white" />
            </shape>
        </clip>
    </item>
</layer-list>